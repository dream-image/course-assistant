{
  "name": "course-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:visualizer": "tsc -b && cross-env VISUALIZER=true vite build &&  open -a \"Google Chrome\" ./dist.html ",
    "lint": "eslint .",
    "preview": "vite preview",
    "commit": "prettier . --check --write &&  eslint src --fix && git add . && git commit && git push",
    "commit:msg": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/pro-components": "^2.8.1",
    "@ant-design/x": "^1.0.5",
    "@heroui/react": "^2.7.4",
    "@react-pdf-viewer/core": "^3.12.0",
    "animate.css": "^4.1.1",
    "antd": "^5.24.0",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.11.11",
    "highlight.js": "^11.11.1",
    "katex": "^0.16.21",
    "lodash-es": "^4.17.21",
    "pdfjs-dist": "^3.4.120",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^6.27.0",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.13.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cz-git": "^1.11.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "less": "^4.2.2",
    "postcss": "^8.4.47",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
